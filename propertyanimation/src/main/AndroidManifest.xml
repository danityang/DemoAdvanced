<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cdemo.propertyanimation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.TweenAnimActivity" />
        <activity android:name=".activity.FrameAnimActivity" />
        <activity android:name=".activity.BasicObjectAnimActivity" />
        <activity android:name=".activity.XmlFileObjectAnimActivity" />
        <activity android:name=".activity.EvaluatorObjectAnimActivity" />
        <activity android:name=".activity.ColorEvaluatorObjectAnimActivity" />
        <activity android:name=".activity.ViewPropertyAnimatorActivity" />
        <activity android:name=".activity.LayoutTransitionAnimActivity" />
    </application>

</manifest>